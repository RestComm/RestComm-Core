	<smtp-notify>
		<host></host>
		<user></user>
		<password></password>
		<port></port>
		<!-- Default mail address used to report issues related to Restcomm management,
		mostly associated with bootstrap executions, like workspace migration, etc. -->
		<default-email-address></default-email-address>
	</smtp-notify>

	<smtp-service>
		<host>
		</host>
		<user></user>
		<password></password>
		<port></port>

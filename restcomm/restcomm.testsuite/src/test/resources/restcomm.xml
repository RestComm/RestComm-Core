			<fax url="localhost:5080" method="SIP" />
			<ussd url="localhost:5080" method="SIP" />
		</callback-urls>
		<voip-innovations>
			<login></login>
			<password></password>
			<endpoint></endpoint>
			<uri>https://backoffice.voipinnovations.com/api2.pl</uri>
